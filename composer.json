{
  "name": "iqual/iq_barrio",
  "description": "Generic barrio sub theme for iqual websites.",
  "type": "drupal-custom-theme",
  "keywords": [
    "Drupal"
  ],
  "homepage": "https://github.com/iqual-ch/iq_barrio",
  "support": {
    "issues": "https://github.com/iqual-ch/iq_barrio/issues",
    "source": "https://github.com/iqual-ch/iq_barrio"
  },
  "require": {
    "php": "^8.1",
    "drupal/core": "^9.5 || ^10",
    "drupal/bootstrap_barrio": "~5.1.0",
    "iqual/iq_barrio_composer_update": "^3.0",
    "iqual/iq_barrio_helper": "^4.0",
    "drupal/pagedesigner_responsive_images": "^3.0 || ^4.0"
  },
  "extra": {
    "patches": {
      "drupal/linkit": {
        "[https://dgo.to/3105061]: Link attributes missing with bootstrap theme": "https://www.drupal.org/files/issues/2024-01-05/linkit-selector-6.1.x-3105061-8.diff"
      },
      "drupal/bootstrap_barrio": {
        "[https://dgo.to/3421337]: Unexpected token \"name\" of value \"if\"": "https://git.drupalcode.org/project/bootstrap_barrio/-/merge_requests/75/diffs.diff?diff_id=758849"
      }
    }
  }
}
