<div class="iq-social-icons" data-align="{{alignment}}">
    {% if href_facebook|render is not empty %}
    <a href="{{href_facebook}}" class="icon-facebook" title="{{title_facebook}}" target="_blank" ><i class="fab fa-facebook-f"></i></a>
    {% endif %}
    {% if href_twitter|render is not empty %}
    <a href="{{href_twitter}}" class="icon-twitter" title="{{title_twitter}}" target="_blank" ><i class="fab fa-twitter"></i></a>
    {% endif %}
    {% if href_linkedin|render is not empty %}
    <a href="{{href_linkedin}}" class="icon-linkedin" title="{{title_linkedin}}" target="_blank" ><i class="fab fa-linkedin-in"></i></a>
    {% endif %}
    {% if href_youtube|render is not empty %}
    <a href="{{href_youtube}}" class="icon-youtube" title="{{title_youtube}}" target="_blank" ><i class="fab fa-youtube"></i></a>
    {% endif %}
    {% if href_instagram|render is not empty %}
    <a href="{{href_instagram}}" class="icon-instagram" title="{{title_instagram}}" target="_blank" ><i class="fab fa-instagram"></i></a>
    {% endif %}
</div>
