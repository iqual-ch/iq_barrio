<div class="iq-svg">
  <div class="{{parent_class}}" >
    {% if href %}
    <a href="{{href}}" title="{{title_link}}" target="{{link_target}}" >
      <img src="{{svg_file}}" alt="{{svg_alt}}" title="{{svg_title}}"/>
    </a>
    {% else %}
      <img src="{{svg_file}}" alt="{{svg_alt}}" title="{{svg_title}}"/>
    {% endif %}
  </div>
</div>
